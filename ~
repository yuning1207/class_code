#include <iostream>
#include <map>
#include <cstring>
using namespace std;

int main(void)
{
	string english,chinese;
	map<string,string> map1;
	map<string,string>::iterator p;
	english="encapsulation";
	chinese="封装性";
	map1.insert(<string,string>::(english,chinese));
	english="inheritance";
	chinese="继承性";
	map1.insert(<string,string>::(english,chinese));
	english="polymorphism";
	chinese="多态性";
	map1.insert(<string,string>::(english,chinese));
	english="message";
	chinese="消息";
	map1.insert(<string,string>::(english,chinese));
	english="class";
	chinese="类";
	map1.insert(<string,string>::(english,chinese));
	english="object";
	chinese="对象";
	map1.insert(<string,string>::(english,chinese));
	return 0;
} 
